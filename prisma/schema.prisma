// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(cuid())
  username String
  email String 
  password String
}

model Student{
  university String
  title String
  college String
  department String
  phoneNumber String
  cField String
  specCource String
  depchoice String
  yearOfStudy String
  regNo String @unique
  residence String
  Guardian String
  gnumber String
  letterUrl String
  uniID String

}